<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN"
        "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>

<!-- bean definitions here -->
	<bean id="savingAccount" class="com.codestrive.appLogic.SavingAccount">
		<constructor-arg value="4%"></constructor-arg>
	</bean>
	<bean id="fixedAccount" class="com.codestrive.appLogic.FixedAccount">
		<constructor-arg value="9%"></constructor-arg>
	</bean>
	<bean id="ppfAccount" class="com.codestrive.appLogic.PPFAccount">
		<constructor-arg value="7.5%"></constructor-arg>
	</bean>
</beans>